{
  "canisters": {
    "chain_cloud": {
      "build": "cargo build --target wasm32-unknown-unknown --package chain_cloud --release",
      "candid": "src/chain_cloud/src/chain_cloud.did",
      "wasm": "target/wasm32-unknown-unknown/release/chain_cloud.wasm",
      "type": "custom"
    },
    "chain_cloud_assets": {
      "dependencies": [
        "chain_cloud"
      ],
      "frontend": {
        "entrypoint": "src/chain_cloud_assets/src/index.html"
      },
      "source": [
        "src/chain_cloud_assets/assets",
        "dist/chain_cloud_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}